#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vincetiu/Documents/github/hackathons/shroomdk/node_modules/.pnpm/@flipsidecrypto+sdk@1.1.1/node_modules/@flipsidecrypto/sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/vincetiu/Documents/github/hackathons/shroomdk/node_modules/.pnpm/@flipsidecrypto+sdk@1.1.1/node_modules/@flipsidecrypto/sdk/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite@4.0.1_@types+node@18.11.13/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../vite@4.0.1_@types+node@18.11.13/node_modules/vite/bin/vite.js" "$@"
fi
